{
  "@import": " url(https://fonts.googleapis.com/css?family=Exo:400,100,200,300,500,600,700,800,900)",
  "font-body": "'Exo', sans-serif",
  "font-heading": "'Exo', serif",
  "base-font-size": "16px",
  "colors": {
    "brand-primary": "#fff",
    "brand-secondary": "#33aec0",
    "brand-tertiary": "#333",
    "border-color": "#eeeeee"
  },
  "base": {
    "boxed-layout": "false",
    "boxed-body-bg": "#EAEAEA",
    "boxed-max-width": "1200px",
    "body-bg": "#FAFAFA",
    "background-image-overlay": "true",
    "background-image-color": "var($colors, brand-tertiary)",
    "background-image-color_opacity": ".4",
    "background-image_paralax": "true",
    "block-margin-padding": "75px"
  },
  "typo": {
    "font-body": "$font-body",
    "font-heading": "$font-heading",
    "body-text": "#444444",
    "body-heading-text": "#333333",
    "base-font-size": "$base-font-size",
    "font-size-h1": "floor($base-font-size * 2.6)",
    "font-size-h2": "floor($base-font-size * 2.15)",
    "font-size-h3": "ceil($base-font-size * 1.7)",
    "font-size-h4": "ceil($base-font-size * 1.25)",
    "font-size-h5": "$base-font-size",
    "font-size-h6": "$base-font-size",
    "heading_font-weight": "700",
    "heading_text-transform": "uppercase",
    "heading_h1h2h3-bottom-margin": "20px",
    "heading_h4h5h6-bottom-margin": "10px",
    "heading_border-type": "'none'",
    "heading_border-color": "var($colors, brand-primary)",
    "heading_border-thickness": "5px",
    "heading_border-length": "50px",
    "heading_border-spacebetween": "15px"
  },
  "menu": {
    "centered_header": "false",
    "height": "82px",
    "background": "#FFF",
    "text": "lighten( var($colors, brand-tertiary), 50% )",
    "text_hover": "var($colors, brand-tertiary)",
    "text-padding": "32px 15px 30px 15px",
    "text-weight": "600",
    "text-size": "14px",
    "text-transform": "uppercase",
    "text-font-family": "var($typo, font-heading)",
    "menu-items-position": "right",
    "brand-name": "var($colors, brand-secondary)",
    "brand-logo": "true",
    "logo-padding-top-bottom": "10px",
    "fixed": "false",
    "transparent-fixed": "false",
    "trans-fixed_text-start-color": "#fff"
  },
  "footer": {
    "padding-top": "var($base, block-margin-padding)",
    "padding-bottom": "var($base, block-margin-padding)",
    "background": "var($colors, brand-secondary)",
    "title": "var($colors, brand-primary)",
    "text": "#FFF",
    "text_hover": "var($colors, brand-primary)",
    "text_font-weight": "300"
  },
  "copyRightFooter": {
    "padding-top": "15px",
    "padding-bottom": "15px",
    "background": "#444444",
    "text": "#999999",
    "text_hover": "#919191",
    "social-icon-size": "22px"
  },
  "button": {
    "border": "false",
    "background": "var($colors, brand-secondary)",
    "background_hover": "darken( var($colors, brand-primary) , 10%)",
    "text": "#FFF",
    "text_hover": "#FFF",
    "borderRadius": "0px"
  },
  "form": {
    "border": "1px solid var($colors, border-color)",
    "border-radius": "0px",
    "placeholder": "#999"
  },
  "pageHeading": {
    "background-color": "var($colors, brand-tertiary)",
    "padding_top-bottom": "80px",
    "color": "#FFF",
    "text-align": "center"
  },
  "blogItem": {
    "header-min-height": "300px",
    "header-max-height": "450px",
    "header-height": "35vh",
    "header-background-color": "var($base, brand-tertiary)",
    "header-margin-bottom": "var($base, block-margin-padding)",
    "header-title-color": "#FFF",
    "header-title-align": "left"
  },
  "carousel": {
    "margin-top": "0px",
    "margin-bottom": "0px",
    "nav-button_show": "true",
    "nav-buttons": "var($colors, brand-primary)",
    "nav-buttons_hover": "var($colors, brand-tertiary)",
    "nav-background": "var($colors, brand-secondary)",
    "nav-background_hover": "darken( var($colors, brand-secondary) , 20%)",
    "nav-border-radius": "0px",
    "nav-button-size": "50px",
    "nav-button-prev_icon": "'\\f104'",
    "nav-button-next_icon": "'\\f105'",
    "nav-offset-from-side": "15px",
    "textblock_h-position": "right",
    "textblock_v-position": "top",
    "textblock_max-width": "900px",
    "textblock_h-sideOffset": "100px",
    "textblock_v-SideOffset": "50px",
    "text": "var($colors, brand-primary)",
    "text-size": "24px",
    "text-transform": "none",
    "text-background": "rgba(0,0,0,0.6)",
    "text-padding": "30px 50px 50px 50px",
    "text_alignment": "left",
    "text-font-family": "var($typo, font-heading)",
    "text-shadow": "0px 0px 3px rgba(0,0,0,.5)",
    "indicator_show": "true",
    "indicator": "var($colors, brand-primary)",
    "indicator_size": "12px",
    "height": "50vh"
  },
  "wysiwyg": {
    "padding-top": "var($base, block-margin-padding)",
    "padding-bottom": "var($base block-margin-padding)"
  },
  "archive": {
    "padding-top": "var($base, block-margin-padding)",
    "padding-bottom": "var($base, block-margin-padding)",
    "title": "var($colors, brand-secondary)",
    "date": "var($colors, brand-tertiary)",
    "image_hover-color": "var($colors brand-tertiary)"
  },
  "welcomeBanner": {
    "padding-top": "35vh",
    "padding-bottom": "35vh",
    "background": "transparent",
    "text": "#ffffff",
    "h1_font-size": "36px",
    "h2_font-size": "20px"
  },
  "uspGrid": {
    "padding-top": "var($base, block-margin-padding)",
    "padding-bottom": "15px",
    "background": "var($colors, brand-secondary)",
    "icon": "var($colors, brand-primary)",
    "title": "white",
    "text_color": "white",
    "icon_size": "4em",
    "margin-bottom": "45px",
    "icon_icon-above-text": "false",
    "icon_border-around-icon": "true",
    "icon_border-color": "#fff",
    "icon_border-background-fill": "transparent",
    "icon_border-radius": "100%",
    "image_border-radius": "50%",
    "image_max-size": "200px"
  },
  "testiominals": {
    "padding-top": "var($base, block-margin-padding)",
    "padding-bottom": "var($base, block-margin-padding)",
    "text": "#FFF",
    "text_alignment": "center",
    "nav_show": "true",
    "nav_position": "'center-bottom'",
    "dots_show": "false",
    "dots-color": "var($colors, brand-primary)",
    "dots-color_active": "white"
  },
  "googleMap": {
    "margin-top": "0px",
    "margin-bottom": "0px",
    "icon-color": "var($colors, brand-secondary)",
    "iconStroke-color": "var($colors, brand-secondary)",
    "height": "450px"
  },
  "projects": {
    "padding-top": "var($base, block-margin-padding)",
    "padding-bottom": "var($base, block-margin-padding)",
    "background-color": "var($colors, brand-secondary)",
    "overlay-color": "var($colors, brand-primary)",
    "heading": "#FFF",
    "text": "#FFF",
    "text_alignment": "center",
    "hide-excerpt-text": "false",
    "show-text-on-hover": "true"
  },
  "imageGallery": {
    "padding-top": "var($base, block-margin-padding)",
    "padding-bottom": "var($base, block-margin-padding)",
    "background-color": "var($colors, brand-secondary)",
    "overlay-color": "var($colors, brand-primary)",
    "text": "#FFF",
    "text_alignment": "center",
    "text_size": "20px",
    "show-text-on-hover": "true"
  },
  "imageGrid": {
    "padding-top": "0px",
    "padding-bottom": "var($base, block-margin-padding)",
    "background-color": "var($colors, brand-secondary)",
    "overlay-color": "var($colors, brand-primary)",
    "text": "#FFF",
    "text_alignment": "center",
    "text_size": "20px",
    "block-height": "100%",
    "show-text-on-hover": "true"
  },
  "textImage": {
    "padding-top": "var($base, block-margin-padding)",
    "padding-bottom": "var($base, block-margin-padding)"
  },
  "breadCrumb": {
    "padding-top": "10px",
    "padding-bottom": "10px",
    "background": "none",
    "text-align": "left",
    "text-size": "12px",
    "text-link-color": "#777",
    "text-color": "#999",
    "item-margin": "5px"
  },
  "cardGrid": {
    "padding-top": "var($base, block-margin-padding)",
    "padding-bottom": "var($base, block-margin-padding)",
    "background_color": "none",
    "card_padding": "20px 20px 30px 20px",
    "card-bg_color": "var($colors, brand-tertiary)",
    "border": "1px solid var($colors, brand-primary)",
    "box-shadow": "0px 0px 5px rgba(0,0,0,0.75)",
    "icon_color": "var($colors, brand-primary)",
    "title_color": "var($colors, brand-primary)",
    "text_color": "var($colors, brand-primary)",
    "text_align": "left",
    "title-border": "true",
    "title-border_width": "80%",
    "title-border_thickness": "5px",
    "title-border_color": "var($colors, brand-primary)",
    "title-border_offset": "30px",
    "icon_size": "100px"
  },
  "enableWoocommerce": "true",
  "wc": {
    "padding-top": "30px",
    "padding-bottom": "var($base, block-margin-padding)",
    "show-breadcrumb": "true",
    "sale-ribbon-bg": "#54C225",
    "sale-ribbon-text": "#333"
  },
  "wc-productPage": {
    "price-color": "#54C225"
  },
  "wc-pagination": {
    "border": "1px solid var($colors, brand-primary)",
    "background": "var($colors, brand-primary)",
    "background_hover": "darken( var($colors, brand-primary) , 10%)",
    "text": "#FFF",
    "text_hover": "#FFF"
  }
}
